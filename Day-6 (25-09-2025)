//String
//ways of getting and displaying string
import java.util.Scanner;
public class Main{
    public  static void main(String[] args){
        char name[] ={'V','S','B'};
        String na = new String(name);
        System.out.println(na);
    }
}

import java.util.Arrays;
public class Main{
    public  static void main(String[] args){
        char name[] ={'V','S','B'};
        String na = Arrays.toString(name);
        System.out.println(na);
    }
}

//length method
public class Main{
    public  static void main(String[] args){
        String str="palcement";
        char [] name ={'a','b'};
        System.out.println(str.length());//in string, we need to give ()
         System.out.println(name.length);//"array is a class" so, we don't give ()
    }
}

//String is immutable
import java.util.Scanner;
public class Main{
    public  static void main(String[] args){
        Scanner kbd=new Scanner(System.in);
        String str=kbd.next(); //(or) new String(kbd.next());
        String str1=kbd.next();
        System.out.println(str.hashCode());
        System.out.println(str1.hashCode());
    }
}

//StringBuilder is mutable
import java.util.Scanner;
public class Main{
    public  static void main(String[] args){
        Scanner kbd=new Scanner(System.in);
        StringBuilder name=new StringBuilder(kbd.next());
        StringBuilder name1=new StringBuilder(kbd.next());
        System.out.println(name.hashCode());
        System.out.println(n1.hashCode());
    }
}

//change Case (Lower& Upper)
import java.util.Scanner;
public class Main{
    public  static void main(String[] args){
        Scanner kbd=new Scanner(System.in);
        String str=kbd.next(); //(or) new String(kbd.next());
        System.out.println(str.toLowerCase());
        System.out.println(str.toUpperCase());
    }
}

//comparasion

import java.util.Scanner;
public class Main{
    public  static void main(String[] args){
        Scanner kbd=new Scanner(System.in);
        String name=kbd.next(); //(or) new String(kbd.next());
        String name1=kbd.next();
        System.out.println(name.equals(name1));
        System.out.println(name.equalsIgnoreCase(name1));
    }
}

import java.util.Scanner;
public class Main{
    public  static void main(String[] args){
        Scanner kbd=new Scanner(System.in);
        String name=kbd.next(); //(or) new String(kbd.next());
        String name1=kbd.next();
        System.out.println(name.equals(name1));
        boolean res=name.equalsIgnoreCase(name1);//it returns the boolean value
        System.out.println(res);
    }
}

import java.util.Scanner;
public class Main{
    public  static void main(String[] args){
        Scanner kbd=new Scanner(System.in);
        String name=kbd.next(); 
        String name1=kbd.next();
        System.out.println(name.compareTo(name1));
    }
}       
